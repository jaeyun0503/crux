// test shadowing of variable

let a: int;

fn shadowWithParam(a: int) {
  a = 7;
  printInt(a);
  println();
}

fn shadowWithLocal() {
  let a: int;
  a = 9;
  printInt(a);
  println();
}

fn main() {
  a = 5;
  printInt(a);
  println();

  shadowWithParam(8);
  printInt(a);
  println();

  shadowWithLocal();
  printInt(a);
  println();
}

