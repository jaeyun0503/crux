
test(a, b) {
  printInt((a + b));
  println();
  printInt((a - b));
  println();
  printInt((a * b));
  println();
  if ((b != 0)) {
    printInt((a / b));
    println();
  } else {
    println();
  }
}
main() {
  i;
  j;
  i = 0;
  while ((i <= 10)) {
    j = 0;
    while ((j <= 10)) {
      test(i, j);
      j = (j + 1);
    }
    i = (i + 1);
  }
}

