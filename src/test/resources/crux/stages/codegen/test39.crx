// test arithmetic

fn test(a: int, b: int) {
  printInt(a + b);
  println();
  printInt(a - b);
  println();
  printInt(a * b);
  println();
  if (b != 0) {
    printInt(a / b);
    println();
  } else {
    println();
  }
}

fn main() {
  let i: int;
  let j: int;
  i = 0;
  while (i <= 10) {
    j = 0;
    while (j <= 10) {
       test(i, j);
       j = j + 1;
    }
    i = i + 1;
  }
}
